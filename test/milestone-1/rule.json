{
  "character_classes": {
    "letter": "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ",
    "digit": "0123456789",
    "_": "_",
    ".": ".",
    "'": "'",
    "{": "{",
    "}": "}",
    "(": "(",
    ")": ")",
    "*": "*",
    "space": " ",
    "tab": "\t",
    "newline": "\n",
    "+": "+",
    "-": "-",
    "e": "e",
    "E": "E",
    "@": "@",
    "$": "$",
    "~": "~",
    "`": "`",
    "#": "#",
    "\\": "\\",
    "|": "|",
    "^": "^",
    "?": "?",
    "!": "!",
    ":": ":",
    "=": "=",
    "<": "<",
    ">": ">",
    ";": ";",
    ",": ",",
    "[": "[",
    "]": "]",
    "/": "/"
  },

  "dfa_config": {
    "start_state": "q0",
    "final_states": [
      "q_identifier",
      "q_number",
      "q_real",
      "q_scientific",
      "q_char_lit",
      "q_string_lit",
      "q_comment",
      "q_plus",
      "q_minus",
      "q_mul",
      "q_div",
      "q_eq",
      "q_lt",
      "q_gt",
      "q_ne",
      "q_let",
      "q_get",
      "q_colon",
      "q_assign",
      "q_dot",
      "q_range",
      "q_semicolon",
      "q_comma",
      "q_lparen",
      "q_rparen",
      "q_lbracket",
      "q_rbracket",
      "q_error"
    ]
  },

  "keyword_lookup": {
    "keywords": [
        "program", "variabel", "mulai", "selesai", "jika", "maka", "selain-itu",
        "selama", "lakukan", "untuk", "ke", "turun-ke", "integer", "real",
        "boolean", "char", "larik", "dari", "prosedur", "fungsi",
        "konstanta", "tipe", "kasus", "rekaman", "ulangi", "sampai"

    ],
    "logical_operators": ["dan", "atau", "tidak"],
    "arithmetic_word_operators": ["bagi", "mod"],
    "boolean_literals": ["benar", "salah"]
  },

  "transitions": [
    {
      "comment": "ERROR HANDLING - Invalid characters like @, $, etc.",
      "rules": [
        {"from": "q0", "input": "@", "to": "q_error"},
        {"from": "q0", "input": "$", "to": "q_error"},
        {"from": "q0", "input": "~", "to": "q_error"},
        {"from": "q0", "input": "`", "to": "q_error"},
        {"from": "q0", "input": "#", "to": "q_error"},
        {"from": "q0", "input": "\\", "to": "q_error"},
        {"from": "q0", "input": "|", "to": "q_error"},
        {"from": "q0", "input": "^", "to": "q_error"},
        {"from": "q0", "input": "?", "to": "q_error"},
        {"from": "q0", "input": "!", "to": "q_error"},
        {"from": "q0", "input": "_", "to": "q_error"}
      ]
    },
    {
      "comment": "IDENTIFIER/KEYWORD - Pattern: letter (letter|digit|_)*",
      "rules": [
        {"from": "q0", "input": "letter", "to": "q_identifier"},
        {"from": "q_identifier", "input": "letter", "to": "q_identifier"},
        {"from": "q_identifier", "input": "digit", "to": "q_identifier"},
        {"from": "q_identifier", "input": "_", "to": "q_identifier"},
        {"from": "q_identifier", "input": "-", "to": "q_identifier"}
      ]
    },
    {
      "comment": "NUMBER - Handles integers and scientific notation (2e3, 2e-3, 2e+3)",
      "rules": [
        {"from": "q0", "input": "digit", "to": "q_number"},
        {"from": "q_number", "input": "digit", "to": "q_number"},
        {"from": "q_number", "input": "e", "to": "q_exp"},
        {"from": "q_number", "input": "E", "to": "q_exp"},
        {"from": "q_exp", "input": "-", "to": "q_exp_sign"},
        {"from": "q_exp", "input": "+", "to": "q_exp_sign"},
        {"from": "q_exp", "input": "digit", "to": "q_scientific"},
        {"from": "q_exp_sign", "input": "digit", "to": "q_scientific"},
        {"from": "q_scientific", "input": "digit", "to": "q_scientific"}
      ]
    },
    {
      "comment": "REAL NUMBER - digit+ '.' digit+",
      "rules": [
        {"from": "q_number", "input": ".", "to": "q_real_start"},
        {"from": "q_real_start", "input": "digit", "to": "q_real"},
        {"from": "q_real", "input": "digit", "to": "q_real"}
      ]
    },

    {
      "comment": "INVALID IDENTIFIER",
      "rules": [
        {"from": "q_number", "input": "letter", "to": "q_error"},
        {"from": "q_number", "input": "_", "to": "q_error"},
        {"from": "q_scientific", "input": "letter", "to": "q_error"},
        {"from": "q_scientific", "input": "_", "to": "q_error"},
        {"from": "q_exp", "input": "letter", "to": "q_error"},
        {"from": "q_exp_sign", "input": "letter", "to": "q_error"}
      ]
    },
    {
      "comment": "CHARACTER LITERAL - Pattern: 'c' (single char inside quotes)",
      "rules": [
        {"from": "q0", "input": "'", "to": "q_char_start"},
        {"from": "q_char_start", "input": "any", "to": "q_char_content"},
        {"from": "q_char_start", "input": "'", "to": "q_char_lit"},
        {"from": "q_char_content", "input": "'", "to": "q_char_lit"}
      ]
    },
    {
      "comment": "STRING LITERAL - Pattern: 'chars+' (handles doubled '' escape)",
      "rules": [
        {"from": "q_char_content", "input": "any", "to": "q_string_content"},
        {"from": "q_string_content", "input": "any", "to": "q_string_content"},
        {"from": "q_string_content", "input": "'", "to": "q_string_lit"}
      ]
    },
    {
      "comment": "ARITHMETIC OPERATOR - + - * / div mod (binary operators)",
      "rules": [
        {"from": "q0", "input": "+", "to": "q_plus"},
        {"from": "q0", "input": "-", "to": "q_minus"},
        {"from": "q0", "input": "*", "to": "q_mul"},
        {"from": "q0", "input": "/", "to": "q_div"},
        
        {"from": "q_scientific", "input": "+", "to": "q_plus"},
        {"from": "q_scientific", "input": "-", "to": "q_minus"},
        {"from": "q_char_lit", "input": "+", "to": "q_plus"},
        {"from": "q_char_lit", "input": "-", "to": "q_minus"},
        {"from": "q_string_lit", "input": "+", "to": "q_plus"},
        {"from": "q_string_lit", "input": "-", "to": "q_minus"}
      ]
    },
    {
      "comment": "RELATIONAL OPERATORS - = < > <= >= <>",
      "rules": [
        {"from": "q0", "input": "=", "to": "q_eq"},
        {"from": "q0", "input": "<", "to": "q_lt"},
        {"from": "q_lt", "input": "=", "to": "q_let"},
        {"from": "q_lt", "input": ">", "to": "q_ne"},
        {"from": "q0", "input": ">", "to": "q_gt"},
        {"from": "q_gt", "input": "=", "to": "q_get"}
      ]
    },
    {
      "comment": "DELIMITERS - : := . .. ; , ( ) [ ]",
      "rules": [
        {"from": "q0", "input": ":", "to": "q_colon"},
        {"from": "q_colon", "input": "=", "to": "q_assign"},
        {"from": "q0", "input": ".", "to": "q_dot"},
        {"from": "q_dot", "input": ".", "to": "q_range"},
        {"from": "q0", "input": ";", "to": "q_semicolon"},
        {"from": "q0", "input": ",", "to": "q_comma"},
        {"from": "q0", "input": ")", "to": "q_rparen"},
        {"from": "q0", "input": "(", "to": "q_lparen"},
        {"from": "q0", "input": "[", "to": "q_lbracket"},
        {"from": "q0", "input": "]", "to": "q_rbracket"}
      ]
    }
  ],

  "state_token_map": {
    "q_identifier": "IDENTIFIER",
    "q_number": "NUMBER",
    "q_real": "REAL_NUMBER",
    "q_scientific": "NUMBER",
    "q_char_lit": "CHAR_LITERAL",
    "q_string_lit": "STRING_LITERAL",
    "q_plus": "ARITHMETIC_OPERATOR",
    "q_minus": "ARITHMETIC_OPERATOR",
    "q_mul": "ARITHMETIC_OPERATOR",
    "q_div": "ARITHMETIC_OPERATOR",
    "q_eq": "RELATIONAL_OPERATOR",
    "q_lt": "RELATIONAL_OPERATOR",
    "q_gt": "RELATIONAL_OPERATOR",
    "q_ne": "RELATIONAL_OPERATOR",
    "q_let": "RELATIONAL_OPERATOR",
    "q_get": "RELATIONAL_OPERATOR",
    "q_colon": "COLON",
    "q_assign": "ASSIGN_OPERATOR",
    "q_dot": "DOT",
    "q_range": "RANGE_OPERATOR",
    "q_semicolon": "SEMICOLON",
    "q_comma": "COMMA",
    "q_lparen": "LPARENTHESIS",
    "q_rparen": "RPARENTHESIS",
    "q_lbracket": "LBRACKET",
    "q_rbracket": "RBRACKET",
    "q_error": "ERROR"
  }
}